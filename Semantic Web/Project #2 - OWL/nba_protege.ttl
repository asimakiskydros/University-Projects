#################################################################
#    Classes
#################################################################

###  http://example.org/nba#ActivePlayer
:ActivePlayer rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :belongsInRoster ;
                                    owl:someValuesFrom :Roster
                                  ] ;
              rdfs:subClassOf :Player ;
              rdfs:comment "Παίκτης που ανήκει στο ρόστερ κάποιας ομάδας." ;
              rdfs:label "Ενεργός παίκτης" .


###  http://example.org/nba#AllStarGame
:AllStarGame rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :playedBy ;
                                   owl:someValuesFrom :AllStarTeam
                                 ] ;
             rdfs:subClassOf :Game ;
             rdfs:comment "Ματς το οποίο γίνεται μια φορά τον χρόνο, όπου συμμετέχουν η πιο πολυψηφισμένοι από το κοινό παίκτες της σεζόν." ;
             rdfs:label "All Star ματς" .


###  http://example.org/nba#AllStarPlayer
:AllStarPlayer rdf:type owl:Class ;
               rdfs:subClassOf :Player ;
               rdfs:label "Πολυψηφισμένος από το κοινό Παίκτης" .


###  http://example.org/nba#AllStarTeam
:AllStarTeam rdf:type owl:Class ;
             rdfs:subClassOf [ owl:intersectionOf ( :Team
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasRoster ;
                                                      owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :hasPlayer ;
                                                                           owl:allValuesFrom :AllStarPlayer
                                                                         ]
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
             rdfs:comment "Ομάδα που συμμετέχει σε All Star ματς, αποτελούμενη από πολυψηφισμένους από το κοινό παίκτες." .


###  http://example.org/nba#AnkleInjury
:AnkleInjury rdf:type owl:Class ;
             rdfs:subClassOf :Injury ;
             owl:disjointWith :GroinInjury ,
                              :KneeInjury ,
                              :ThighInjury ;
             rdfs:label "Τραυματισμός αστραγάλου" .


###  http://example.org/nba#Arena
:Arena rdf:type owl:Class ;
       rdfs:subClassOf rdfs:Resource ;
       rdfs:comment "Αντιπροσωπεύει το γήπεδο-έδρα της αντίστοιχης ομάδας." ;
       rdfs:label "Το γήπεδο της ομάδας." .


###  http://example.org/nba#AssistantCoach
:AssistantCoach rdf:type owl:Class ;
                rdfs:subClassOf :Coach ;
                rdfs:label "Βοηθητικός Προπονητής" .


###  http://example.org/nba#Award
:Award rdf:type owl:Class ;
       rdfs:subClassOf rdfs:Resource ;
       rdfs:comment "Διάφοροι τίτλοι που έχουν κερδίσει παίκτες, όπως MVP σε Finals ή Season, Defensive Player of the Year, Rookie of the Year." ;
       rdfs:label "Τίτλοι-βραβεία παικτών." .


###  http://example.org/nba#BigArena
:BigArena rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :capacity ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:integer ;
                                                     owl:withRestrictions ( [ xsd:minInclusive 20000
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
          rdfs:subClassOf [ owl:intersectionOf ( :Arena
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :capacity ;
                                                   owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                       owl:onDatatype xsd:integer ;
                                                                       owl:withRestrictions ( [ xsd:minInclusive 20000
                                                                                              ]
                                                                                            )
                                                                     ]
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
          rdfs:label "Στάδιο αυξημένης χωρητικότητας" .


###  http://example.org/nba#Coach
:Coach rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       owl:disjointWith :Medic ,
                        :Official ,
                        :Player ,
                        :Trainer ;
       rdfs:comment "Οι προπονητές χωρίζονται σε επικεφαλείς και βοηθητικούς." ;
       rdfs:label "Προπονητής" .


###  http://example.org/nba#CoachingStaff
:CoachingStaff rdf:type owl:Class ;
               rdfs:subClassOf rdfs:Resource ;
               owl:disjointWith :MedicalTeam ,
                                :Roster ;
               rdfs:comment "Το σύνολο των προπονητών μιας ομάδας, είτε επικαφαλείς είτε βοηθητικοί." ;
               rdfs:label "Προπονητικό Επιτελείο." .


###  http://example.org/nba#Conference
:Conference rdf:type owl:Class ;
            rdfs:subClassOf rdfs:Resource ;
            rdfs:comment "Το NBA χωρίζεται σε 2 μεγάλες περιφέρειες, την Ανατολική και τη Δυτική, ανάλογα με την τοποθεσία της κάθε ομάδας. Κάθε μια περιέχει ισάριθμο πλήθος ομάδων." ;
            rdfs:label "Περιφέρεια" .


###  http://example.org/nba#Contract
:Contract rdf:type owl:Class ;
          rdfs:subClassOf rdfs:Resource ;
          rdfs:comment "Συμβόλαια μεταγραφών που υπογράφουν οι παίκτες με τις ομάδες." ;
          rdfs:label "Συμβόλαια των παικτών." .


###  http://example.org/nba#CriticalInjury
:CriticalInjury rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:unionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :severity ;
                                                      owl:hasValue 10
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :severity ;
                                                      owl:hasValue 8
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :severity ;
                                                      owl:hasValue 9
                                                    ]
                                                  )
                                    ] ;
                rdfs:subClassOf :Injury ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :treatedBy ;
                                  owl:allValuesFrom :ER_Doctor
                                ] ;
                rdfs:comment "Τραυματισμός βαθμού άνω του 8, απαιτεί άμεση αντιμετώπιση." ;
                rdfs:label "Κρίσιμος τραυματισμός" .


###  http://example.org/nba#DPOY
:DPOY rdf:type owl:Class ;
      rdfs:subClassOf :Award ;
      rdfs:label "Ο καλύτερος αμυντικός παίκτης της χρονιάς." .


###  http://example.org/nba#Division
:Division rdf:type owl:Class ;
          rdfs:subClassOf rdfs:Resource ;
          rdfs:comment "Κάθε περιφέρεια περιέχει συγκεκριμένες κατηγορίες ισοπλήθη σε ομάδες που ανταγωνίζονται μεταξύ τους." ;
          rdfs:label "Κατηγορία ομάδων." .


###  http://example.org/nba#DominantTeam
:DominantTeam rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :championships ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:integer ;
                                                         owl:withRestrictions ( [ xsd:minInclusive 5
                                                                                ]
                                                                              )
                                                       ]
                                  ] ;
              rdfs:subClassOf [ owl:intersectionOf ( :Team
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :championships ;
                                                       owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                           owl:onDatatype xsd:integer ;
                                                                           owl:withRestrictions ( [ xsd:minInclusive 5
                                                                                                  ]
                                                                                                )
                                                                         ]
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
              rdfs:comment "Ομάδα με μεγάλο αριθμό κερδισμένων πρωταθλημάτων." ;
              rdfs:label "Κυρίαρχη Ομάδα" .


###  http://example.org/nba#ER_Doctor
:ER_Doctor rdf:type owl:Class ;
           rdfs:subClassOf :Medic ;
           rdfs:label "Γιατρός Επειγόντων Περιστατικών." .


###  http://example.org/nba#ER_MedicalTeam
:ER_MedicalTeam rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasMedic ;
                                      owl:someValuesFrom :ER_Doctor
                                    ] ;
                rdfs:subClassOf [ owl:intersectionOf ( :MedicalTeam
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasMedic ;
                                                         owl:allValuesFrom :ER_Doctor
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
                rdfs:comment "Αποτελείται από γιατρούς που εξειδικεύονται σε περιπτώσεις έκτακτης ανάγκης." ;
                rdfs:label "Ομάδα Επειγόντων Περιστατικών." .


###  http://example.org/nba#FinalsMVP
:FinalsMVP rdf:type owl:Class ;
           rdfs:subClassOf :Award ;
           rdfs:label "Ο πιο σημαντικός παίκτης των τελικών." .


###  http://example.org/nba#Game
:Game rdf:type owl:Class ;
      rdfs:subClassOf rdfs:Resource ;
      rdfs:comment "Παιχνίδι της αγωνιστικής μεταξύ ομάδων." ;
      rdfs:label "Ματς" .


###  http://example.org/nba#GroinInjury
:GroinInjury rdf:type owl:Class ;
             rdfs:subClassOf :Injury ;
             owl:disjointWith :KneeInjury ,
                              :ThighInjury ;
             rdfs:label "Τραυματισμός βουβονικής περιοχής" .


###  http://example.org/nba#HeadCoach
:HeadCoach rdf:type owl:Class ;
           rdfs:subClassOf :Coach ;
           rdfs:label "Βασικός ή Επικεφαλής προπονητής" .


###  http://example.org/nba#HighScorer
:HighScorer rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasStatistics ;
                                  owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                       owl:onProperty :points ;
                                                       owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                            owl:onDatatype xsd:decimal ;
                                                                            owl:withRestrictions ( [ xsd:minInclusive 20
                                                                                                   ]
                                                                                                 )
                                                                          ]
                                                     ]
                                ] ;
            rdfs:subClassOf [ owl:intersectionOf ( :Player
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasStatistics ;
                                                     owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :points ;
                                                                          owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                              owl:onDatatype xsd:decimal ;
                                                                                              owl:withRestrictions ( [ xsd:minInclusive 20
                                                                                                                     ]
                                                                                                                   )
                                                                                            ]
                                                                        ]
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
            rdfs:comment "τα βάζει τα σουτ" ;
            rdfs:label "Παίκτης με υψηλό μέσο όρο πόντων" .


###  http://example.org/nba#HighValueContract
:HighValueContract rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :moneyValue ;
                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                              owl:onDatatype xsd:integer ;
