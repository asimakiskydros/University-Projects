@base <http://example.org/nba#> .

<http://example.org/nba> rdf:type owl:Ontology ;
                          spin:imports <http://topbraid.org/spin/rdfsplus> ;
                          owl:versionInfo "Created with TopBraid Composer" .

#################################################################
#    Annotation properties
#################################################################

###  http://spinrdf.org/spin#imports
spin:imports rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/nba#belongsInCoachingStaff
:belongsInCoachingStaff rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :partOf ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain :Coach ;
                        rdfs:range :CoachingStaff .


###  http://example.org/nba#belongsInMedicalTeam
:belongsInMedicalTeam rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :partOf ;
                      owl:inverseOf :hasMedic ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:domain :Medic ;
                      rdfs:range :MedicalTeam .


###  http://example.org/nba#belongsInRoster
:belongsInRoster rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :partOf ;
                 owl:inverseOf :hasPlayer ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Player ;
                 rdfs:range :Roster .


###  http://example.org/nba#coachedBy
:coachedBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :coaches ;
           rdfs:domain :Roster ;
           rdfs:range :Coach .


###  http://example.org/nba#coaches
:coaches rdf:type owl:ObjectProperty .


###  http://example.org/nba#contractor
:contractor rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Contract ;
            rdfs:range :Team ;
            rdfs:label "Η ομάδα που υπέγραψε το συμβόλαιο με τον παίκτη." .


###  http://example.org/nba#coworkerOf
:coworkerOf rdf:type owl:ObjectProperty ,
                     owl:SymmetricProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :Person ;
            rdfs:range :Person .


###  http://example.org/nba#hasCoachingStaff
:hasCoachingStaff rdf:type owl:ObjectProperty ;
                  rdfs:domain :Team ;
                  rdfs:range :CoachingStaff .


###  http://example.org/nba#hasHomeTurf
:hasHomeTurf rdf:type owl:ObjectProperty ;
             rdfs:domain :Team ;
             rdfs:range :Arena .


###  http://example.org/nba#hasInjury
:hasInjury rdf:type owl:ObjectProperty ;
           rdfs:domain :Player ;
           rdfs:range :Injury .


###  http://example.org/nba#hasMedic
:hasMedic rdf:type owl:ObjectProperty .


###  http://example.org/nba#hasMedicalTeam
:hasMedicalTeam rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain :Team ;
                rdfs:range :MedicalTeam .


###  http://example.org/nba#hasOfficial
:hasOfficial rdf:type owl:ObjectProperty ;
             rdfs:domain :Game ;
             rdfs:range :Official .


###  http://example.org/nba#hasPlayer
:hasPlayer rdf:type owl:ObjectProperty .


###  http://example.org/nba#hasRoster
:hasRoster rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ,
                    owl:InverseFunctionalProperty ;
           rdfs:domain :Team ;
           rdfs:range :Roster .


###  http://example.org/nba#hasStatistics
:hasStatistics rdf:type owl:ObjectProperty ;
               rdfs:domain :Player ;
               rdfs:range :Statistics .


###  http://example.org/nba#hasWonAward
:hasWonAward rdf:type owl:ObjectProperty ;
             rdfs:domain :Player ;
             rdfs:range :Award .


###  http://example.org/nba#hosted
:hosted rdf:type owl:ObjectProperty ;
        rdfs:domain :Arena ;
        rdfs:range :Game .


###  http://example.org/nba#partOf
:partOf rdf:type owl:ObjectProperty .


###  http://example.org/nba#partOfConference
:partOfConference rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :partOf ;
                  rdfs:domain :Division ;
                  rdfs:range :Conference .


###  http://example.org/nba#partOfDivision
:partOfDivision rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :partOf ;
                rdfs:domain :Team ;
                rdfs:range :Division .


###  http://example.org/nba#partOfSeason
:partOfSeason rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :partOf ;
              rdfs:domain :Game ;
              rdfs:range :Season .


###  http://example.org/nba#playedBy
:playedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :playedIn .


###  http://example.org/nba#playedIn
:playedIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Team ;
          rdfs:range :Game .


###  http://example.org/nba#rivalOf
:rivalOf rdf:type owl:ObjectProperty ,
                  owl:TransitiveProperty ;
         rdfs:domain :Team ;
         rdfs:range :Team .


###  http://example.org/nba#sameDivisionWith
:sameDivisionWith rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain :Team ;
                  rdfs:range :Team .


###  http://example.org/nba#siblingOf
:siblingOf rdf:type owl:ObjectProperty ,
                    owl:SymmetricProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain :Person ;
           rdfs:range :Person .


###  http://example.org/nba#signedContract
:signedContract rdf:type owl:ObjectProperty ;
                rdfs:domain :Player ;
                rdfs:range :Contract .


###  http://example.org/nba#teammateOf
:teammateOf rdf:type owl:ObjectProperty ,
                     owl:SymmetricProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :Player ;
            rdfs:range :Player .


###  http://example.org/nba#tradedWith
:tradedWith rdf:type owl:ObjectProperty ,
                     owl:SymmetricProperty ;
            rdfs:domain :Team ;
            rdfs:range :Team .


###  http://example.org/nba#trainedBy
:trainedBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :trains ;
           rdfs:domain :Roster ;
           rdfs:range :Trainer .


